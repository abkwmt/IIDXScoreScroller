{
	"name": "score_scroller",
	"description" : "score_scroller",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": [
		"storage"
	],
	"page_action":{
		"default_icon": {
			"16": "icon.png"
		},
		"show_matches":["https://textage.cc/score/*"]
	},
	"content_scripts":[
		{
			"matches":[
				"https://textage.cc/score/*"
			],
			"js":["score_scroller.js"],
			"css":["score_scroller.css"]
		}
	]
}
